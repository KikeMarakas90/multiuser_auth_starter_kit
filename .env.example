# .env.example - Example environment variables
# Replace the values with your local credentials before running the app

# SECRET_KEY: Secret key for JWT (do NOT commit real secrets)
SECRET_KEY=YOUR_SECRET_KEY_HERE

# ALGORITHM: Algorithm for JWT encoding (default HS256)
ALGORITHM=HS256

# TOKEN_EXPIRE_MINUTES: Token expiration time in minutes
TOKEN_EXPIRE_MINUTES=60
